#if ($stylists)
<ul>
    #foreach ($stylist in $stylists)
    <li><a href="/stylists/stylist.getId()"> Current stylist:</a></li>
    <li><a href="/stylists/stylist.getId()">$stylist.getFirstName.</a></li>
    <li><a href="/stylists/stylist.getId()">$stylist.getLastName.</a></li>
    <li><a href="/stylists/stylist.getId()">$stylist.getPhoneNumber.</a></li>
    #end
</ul>
        #else
<h1>No stylist</h1>
        #end
<a href="/stylists/new">Add new stylist</a>